@page
@model Fase4_WebExterna.Pages.Account.EditProfileModel

<h1>Editar Perfil</h1>

<form method="post">

    <div>
        <label>Nombre</label>
        <input asp-for="Input.Nom" />
    </div>

    <div>
        <label>Cognoms</label>
        <input asp-for="Input.Cognoms" />
    </div>

    <button type="submit">Guardar cambios</button>

    <p style="color:green">@Model.SuccessMessage</p>
    <p style="color:red">@Model.ErrorMessage</p>

</form>
